    function SaveODDEven() {


        $("#mName").removeClass('input-validation-error');
        $("#mMobileNo").removeClass('input-validation-error');
        $("#mCity").removeClass('input-validation-error');

        var flag = false;
        if ($("#mName").val().trim() == '') {
            $("#mName").addClass('input-validation-error');
            flag = true;
        }
        if ($("#mMobileNo").val().trim() == '') {
            $("#mMobileNo").addClass('input-validation-error');
           flag = true;
        }
        if ($("#mEmail").val().trim() == '') {
            $("#mEmail").addClass('input-validation-error');
            flag = true;
        }
         if ($("#mORG").val().trim() == '') {
            $("#mORG").addClass('input-validation-error');
            flag = true;
        }
          if ($("#mComments").val().trim() == '') {
            $("#mComments").addClass('input-validation-error');
            flag = true;
        }



        if (flag)
            return;

        // Save In DB


             $.ajax({
                 url: '@Url.Action("SaveOddEvenDetails","Home")',
                 method:'POST',
                data: {
                    'Name': $("#mName").val(),
                    'MobileNo': $("#mMobileNo").val(),
                    'Email': $("#mEmail").val(),
                    'ORG': $("#mORG").val(),
                    'Comments': $("#mComments").val(),



               },
            beforeSend: function () {

            },
            complete: function () {

            },
             success: function (data) {

                 $("#btnClose").click();

                 $("#mName").val('');
                 $("#mMobileNo").val('');
                 $("#mEmail").val('');
                 $("#mORG").val('');
                 $("#mComments").val('');


                alert(data);

            }
        });


    }



    function OpenModel() {
        $("#myModal").modal(

            { backdrop: 'static' }


        );

    }

